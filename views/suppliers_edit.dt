extends layout_default

block contents
	article.container
		header.row
			.col-12
				h2& Edit Suppliers
		section.row
			form(action="/suppliers", method="PUT")
                .mb-3
                    input(type="hidden", name="_id", value="#{supplier._id}")
                include suppliers
                .mb-3
					button.btn.btn-secondary(onclick="javascript::window.close()")& Cancel
					button.btn.btn-primary(type="submit")& Confirm
